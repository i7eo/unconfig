{
  "name": "@unconfig/constants",
  "version": "0.1.0",
  "description": "ðŸš€ The package offer constants for @unconfig!",
  "keywords": [
    "unconfig",
    "monorepo",
    "pnpm",
    "turborepo",
    "changesets",
    "vite",
    "vitest",
    "constants"
  ],
  "license": "MIT",
  "author": "i7eo <admin@i7eo.com> (https://github.com/i7eo)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup ./src/index.ts --format esm,cjs --dts --watch",
    "build": "rimraf dist && tsup ./src/index.ts --format esm,cjs --dts --target=node18",
    "lint": "run-s lint:**",
    "lint:eslint": "TIMING=1 eslint \"*.{js,jsx,ts,tsx,md,json}\" --fix",
    "lint:prettier": "prettier --write \"*.{js,ts,jsx,tsx,md,json}\"",
    "clean": "run-p clean:**",
    "clean:cache": "rimraf .turbo && rimraf dist",
    "clean:deps": "rimraf node_modules"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.6"
  }
}
