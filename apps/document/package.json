{
  "name": "@unconfig/document",
  "version": "0.1.2",
  "description": "ðŸš€ The package offer document for @unconfig!",
  "type": "module",
  "keywords": [
    "vue",
    "monorepo",
    "pnpm",
    "turborepo",
    "changesets",
    "vite",
    "document"
  ],
  "license": "MIT",
  "author": "i7eo",
  "scripts": {
    "tailwindcss:init": "pnpm exec tailwindcss init",
    "dev": "DEV=true NODE_OPTIONS=\"--import tsx/esm\" pnpm exec vitepress dev",
    "dev:max-size": "DEV=true NODE_OPTIONS=\"--import tsx/esm\" pnpm exec vitepress dev --max-old-space-size=50000",
    "build": "DEV=true NODE_OPTIONS=\"--import tsx/esm\" pnpm exec vitepress build",
    "preview": "DEV=true NODE_OPTIONS=\"--import tsx/esm\" pnpm exec vitepress serve",
    "lint": "run-s \"lint:**\"",
    "lint:prettier": "prettier . --write",
    "clean": "run-s \"clean:**\"",
    "clean:cache": "rimraf .turbo && rimraf dist",
    "clean:deps": "rimraf node_modules"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@unconfig/github": "workspace:*",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "medium-zoom": "^1.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@unconfig/meta": "workspace:*",
    "@vite-pwa/vitepress": "^0.5.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.3",
    "fast-glob": "^3.3.2",
    "feed": "^4.2.2",
    "markdown-it": "^14.1.0",
    "pagefind": "^1.1.0",
    "postcss": "^8.4.38",
    "sitemap-ts": "^1.7.3",
    "typescript": "^5.5.2",
    "unplugin-vue-components": "^0.27.1",
    "vite": "^5.3.1",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-pwa": "^0.20.0",
    "vitepress": "^1.2.3",
    "workbox-window": "^7.1.0"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
